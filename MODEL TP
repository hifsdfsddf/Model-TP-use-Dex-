-- Variables
local noclipEnabled = false
local noclipConnection = nil
local results = {}

-- Functions
local function clearResults()
    for _, child in pairs(SearchResults:GetChildren()) do
        if child:IsA("TextButton") then
            child:Destroy()
        end
    end
end

local function addResult(name, path)
    local resultBtn = Instance.new("TextButton")
    resultBtn.Name = "Result_" .. #results
    resultBtn.Parent = SearchResults
    resultBtn.BackgroundColor3 = Color3.fromRGB(60, 60, 70)
    resultBtn.BorderSizePixel = 0
    resultBtn.Size = UDim2.new(1, -10, 0, 30)
    resultBtn.Font = Enum.Font.Gotham
    resultBtn.Text = name .. " ‚Üí " .. path
    resultBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
    resultBtn.TextSize = 11
    resultBtn.TextXAlignment = Enum.TextXAlignment.Left
    
    resultBtn.MouseButton1Click:Connect(function()
        local char = player.Character
        if char and char:FindFirstChild("HumanoidRootPart") then
            local target = workspace:FindFirstChild(name, true)
            if target and target:IsA("BasePart") then
                char.HumanoidRootPart.CFrame = target.CFrame + Vector3.new(0, 5, 0)
                StatusLabel.Text = "‚úÖ Teleported to " .. name
                StatusLabel.TextColor3 = Color3.fromRGB(0, 255, 100)
            else
                StatusLabel.Text = "‚ùå Target not found: " .. name
                StatusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
            end
        end
    end)
    
    resultBtn.MouseEnter:Connect(function()
        resultBtn.BackgroundColor3 = Color3.fromRGB(80, 80, 90)
    end)
    
    resultBtn.MouseLeave:Connect(function()
        resultBtn.BackgroundColor3 = Color3.fromRGB(60, 60, 70)
    end)
end

-- Search Function
SearchButton.MouseButton1Click:Connect(function()
    local searchTerm = SearchBox.Text:lower()
    if searchTerm == "" then
        StatusLabel.Text = "‚ùå Enter search term"
        StatusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
        return
    end
    
    clearResults()
    results = {}
    
    StatusLabel.Text = "üîç Searching..."
    StatusLabel.TextColor3 = Color3.fromRGB(255, 165, 0)
    
    for _, obj in pairs(workspace:GetDescendants()) do
        if obj:IsA("BasePart") and obj.Name:lower():find(searchTerm) then
            table.insert(results, {obj.Name, obj:GetFullName()})
            addResult(obj.Name, obj:GetFullName())
        end
    end
    
    SearchResults.CanvasSize = UDim2.new(0, 0, 0, ResultsLayout.AbsoluteContentSize.Y)
    
    if #results == 0 then
        StatusLabel.Text = "‚ùå No parts found"
        StatusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
    else
        StatusLabel.Text = "‚úÖ Found " .. #results .. " parts"
        StatusLabel.TextColor3 = Color3.fromRGB(0, 255, 100)
    end
end)

-- Button Functions
IYButton.MouseButton1Click:Connect(function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
    StatusLabel.Text = "üåü Infinite Yield loaded!"
    StatusLabel.TextColor3 = Color3.fromRGB(0, 255, 100)
end)

DexButton.MouseButton1Click:Connect(function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Babyhamsta/Roblox/main/Universal/BypassedDarkDexV3.lua"))()
    StatusLabel.Text = "üîç Dex Explorer loaded!"
    StatusLabel.TextColor3 = Color3.fromRGB(0, 255, 100)
end)

local noclipEnabled = false
NoclipButton.MouseButton1Click:Connect(function()
    noclipEnabled = not noclipEnabled
    NoclipButton.Text = noclipEnabled and "‚úÖ Noclip ON" or "üëª Noclip"
    NoclipButton.BackgroundColor3 = noclipEnabled and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 100, 100)
    
    if noclipEnabled then
        noclipConnection = RunService.Stepped:Connect(function()
            local char = player.Character
            if char then
                for _, part in pairs(char:GetDescendants()) do
                    if part:IsA("BasePart") and part.CanCollide then
                        part.CanCollide = false
                    end
                end
            end
        end)
    else
        if noclipConnection then
            noclipConnection:Disconnect()
        end
    end
end)

FlingButton.MouseButton1Click:Connect(function()
    local char = player.Character
    if char and char:FindFirstChild("HumanoidRootPart") then
        char.HumanoidRootPart.Velocity = Vector3.new(0, 5000, 0)
        StatusLabel.Text = "üí• Flinged!"
        StatusLabel.TextColor3 = Color3.fromRGB(255, 100, 255)
    end
end)

ToggleButton.MouseButton1Click:Connect(function()
    MainFrame.Visible = not MainFrame.Visible
    ToggleButton.Text = MainFrame.Visible and "üëÅÔ∏è Hide GUI" or "üëÅÔ∏è Show GUI"
end)

CloseButton.MouseButton1Click:Connect(function()
    if noclipConnection then
        noclipConnection:Disconnect()
    end
    ScreenGui:Destroy()
end)

-- Keyboard shortcuts
UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end
    
    if input.KeyCode == Enum.KeyCode.RightShift then
        MainFrame.Visible = not MainFrame.Visible
    elseif input.KeyCode == Enum.KeyCode.F1 then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
    end
end)

print("üß† IQ OBBY HUB v2.0 ULTIMATE LOADED!")
print("üì± Right Shift = Toggle GUI")
print("üåü F1 = Infinite Yield")
print("üîç Search any part ‚Üí Click result to TP!")
